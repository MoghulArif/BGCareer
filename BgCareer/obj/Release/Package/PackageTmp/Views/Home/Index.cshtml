                        <form method="post" enctype="multipart/form-data">

                            @*====================NAME==================================*@

                            <div class="row">
                                <div class="form-group">
                                    <label for="exampleInputEmail1" class="col-lg-4" id="cNameLabel">NAME:</label>

                                    <input type="text" class="form-control col-lg-8" id="cName" placeholder="Enter name" onkeydown="return alphaOnly(event);">
                                    @*<small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>*@
                                </div>
                            </div>
                            <br />

                            @*====================MOBILE NUMBER==================================*@


                            <div class="row">
                                <div class="form-group">
                                    <label for="exampleInputPassword1" class="col-lg-2" id="cNameLabel">MOBILE:</label>
                                    <input type="tel" class="form-control col-lg-10" id="cPhoneNumber" placeholder="05********"
                                           onkeydown="return numOnly(e);" maxlength="12">
                                </div>
                            </div>
                            <br />

                            @*====================EMAIL ADDRESS==================================*@

                            <div class="row">
                                <div class="form-group">
                                    <label for="exampleInputPassword1" class="col-lg-2" id="cNameLabel">EMAIL:</label>
                                    <input type="email" class="form-control col-lg-10" id="cEmail" placeholder=" email@company.com" onchange="validate()" />
                                </div>
                            </div>
                            <br />


                            @*====================POSITION APPLIED FOR==================================*@


                            <div class="row">
                                <div class="form-group">
                                    <label for="exampleInputPassword1" class="col-lg-2" id="cNameLabel">POSITION:&nbsp;</label>
                                    <select class="col-lg-10 cPositionClass required" name="cPosition" id="cPosition">
                                        <option value="">Select your occupation</option>
                                        <option value="Software Engineer">Software Engineer</option>
                                        <option value="Graphic Designer">Graphic Designer</option>
                                        <option value="Marketing Manager">Marketing Manager</option>
                                        <option value="Others">Others</option>
                                    </select><br />
                                </div>
                            </div>
                            <br />


                            @*====================ATTACHMENT CV==================================*@


                            <div class="row">
                                <div class="form-group">
                                    <label for="exampleInputPassword1" class="col-lg-2" id="cNameLabel">ATTACH CV:</label>
                                    <input type="file" id="FileUpload1" name="file" accept="application/pdf" onchange="FileSelected(this)" style="width:1000px; font-family: Algerian; font-size:medium; " />
                                    <small id="emailHelp" class="form-text text-muted">We accept only PDF.</small>
                                </div>
                            </div>
                            <br />

                            @*====================FILE UPLOAD ERROR==================================*@


                            <div id="fileuploaderror" style="color:red;display:none"></div>


                            @*====================SUBMIT BUTTON==================================*@

                            <div class="row">
                                <div class="form-group">
                                    <label class="col-lg-1"></label>
                                    <input id="cSubmit" class="col-lg-11" type="submit" onclick="fileUpload()" value="SUBMIT" style="font-family:'Adobe Arabic'; font-size:large;" />
                                </div>
                            </div>
                            <br />                           
                        </form>
